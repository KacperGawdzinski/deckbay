<% if( (typeof error_message  == 'undefined') ) { var error_message = '' } %>
<% if( (typeof user_login  == 'undefined') ) { var user_login = '' } %>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href= "/public/stylesheets/page.css" rel="stylesheet" type="text/css" />
    <link href= "/public/stylesheets/game_list.css" rel="stylesheet" type="text/css" />
    <title><%= game_type %></title>
</head>

<%- include('components/header.ejs', { err_message : `${error_message}`, user_login : `${user_login}` }); %>

<body class="content">

    <div class="container">
        <div class="list">
            <div class="list_header">
                <button id="add_btn" class="add_btn">+</button>
                <p id="game_type"><%= game_type %> ROOM LIST</p>
            </div>
            <%- include('components/new_room.ejs', {game: `${game_type}`}) %>
            <div id="rooms"></div>
        </div>
    </div>

</body>
<footer>
    2020-2021 | Deckbay by GBS
</footer>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script src="/public/javascripts/jquery.redirect.js"></script>
<script src="/socket.io/socket.io.js"></script>
<script src="/public/javascripts/colors.js"></script>
<script src="/public/javascripts/room-lists/room_list.js"></script>
<script src="/public/javascripts/room-lists/chess_rl.js"></script>
<script src="/public/javascripts/modal_handler.js"></script>
<script src="/public/javascripts/mobile_header.js"></script>
</html>